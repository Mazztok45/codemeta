@prefix dc1: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix schema1: <http://schema.org/> .
@prefix semapv: <https://w3id.org/semapv/vocab/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sssom: <https://w3id.org/sssom/> .
@prefix target: <https://w3id.org/codemeta/crosswalks/bibtex/vocab> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sssom:confidence a owl:AnnotationProperty .

sssom:mapping_justification a owl:AnnotationProperty .

sssom:object_label a owl:AnnotationProperty .

sssom:subject_label a owl:AnnotationProperty .

schema1:affiliation skos:exactMatch target:organization .

schema1:author skos:exactMatch target:author .

schema1:codeRepository skos:exactMatch target:repository .

schema1:description skos:exactMatch target:abstract .

schema1:downloadUrl skos:exactMatch target:file .

schema1:editor skos:exactMatch target:editor .

schema1:license skos:exactMatch target:license .

schema1:publisher skos:exactMatch target:publisher .

schema1:releaseNotes skos:exactMatch target:note .

schema1:softwareVersion skos:exactMatch target:version .

schema1:url skos:exactMatch target:url .

schema1:identifier skos:broader target:doi,
        target:hal_id .

schema1:datePublished skos:closeMatch target:month,
        target:year ;
    skos:exactMatch target:date .

[] a owl:Axiom ;
    owl:annotatedProperty skos:closeMatch ;
    owl:annotatedSource schema1:datePublished ;
    owl:annotatedTarget target:year ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "year" ;
    sssom:subject_label "datePublished" .

