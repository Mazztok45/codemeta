@prefix codemeta: <https://codemeta.github.io/terms/> .
@prefix dc1: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix semapv: <https://w3id.org/semapv/vocab/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sssom: <https://w3id.org/sssom/> .
@prefix target: <https://w3id.org/codemeta/crosswalks/bibtex/vocab> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sssom:confidence a owl:AnnotationProperty .

sssom:mapping_justification a owl:AnnotationProperty .

sssom:object_label a owl:AnnotationProperty .

sssom:object_source a owl:AnnotationProperty .

sssom:subject_label a owl:AnnotationProperty .

sssom:subject_source a owl:AnnotationProperty .

codemeta:affiliation skos:exactMatch target:organization .

codemeta:author skos:exactMatch target:author .

codemeta:codeRepository skos:exactMatch target:repository .

codemeta:description skos:exactMatch target:abstract .

codemeta:downloadUrl skos:exactMatch target:file .

codemeta:editor skos:exactMatch target:editor .

codemeta:license skos:exactMatch target:license .

codemeta:publisher skos:exactMatch target:publisher .

codemeta:releaseNotes skos:exactMatch target:note .

codemeta:softwareVersion skos:exactMatch target:version .

codemeta:url skos:exactMatch target:url .

codemeta:identifier skos:exactMatch target:doi,
        target:hal_id .

codemeta:datePublished skos:closeMatch target:month ;
    skos:exactMatch target:date,
        target:year .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource codemeta:releaseNotes ;
    owl:annotatedTarget target:note ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "note" ;
    sssom:object_source <target> ;
    sssom:subject_label "releaseNotes" ;
    sssom:subject_source <schema> .

[] a owl:Ontology ;
    dc1:description "A description of the mapping." ;
    dc1:license "https://creativecommons.org/publicdomain/zero/1.0/"^^xsd:anyURI ;
    pav:authoredOn "2025-01-29"^^xsd:date ;
    sssom:mapping_provider "https://www.softwareheritage.org/."^^xsd:anyURI ;
    sssom:mapping_set_id "https://w3id.org/codemeta/crosswalks/bibtex"^^xsd:anyURI .

