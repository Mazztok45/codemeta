@prefix codemeta: <https://w3id.org/codemeta/3.0> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix schema1: <http://schema.org/> .
@prefix semapv: <https://w3id.org/semapv/vocab/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sssom: <https://w3id.org/sssom/> .
@prefix subject: <https://swmath.org/vocabulary/codemeta-mapping/vocab> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sssom:confidence a owl:AnnotationProperty .

sssom:mapping_justification a owl:AnnotationProperty .

sssom:object_label a owl:AnnotationProperty .

sssom:subject_label a owl:AnnotationProperty .

subject:classification skos:broader codemeta:classification .

subject:description skos:exactMatch schema1:description .

subject:homepage skos:exactMatch codemeta:homepage .

subject:id skos:exactMatch codemeta:id .

subject:name skos:exactMatch schema1:name .

subject:references skos:exactMatch codemeta:references .

subject:references_year_alt skos:closeMatch codemeta:references_year_alt .

<https://swmath.org/vocabulary/codemeta-mapping/vocabrelated_software/id> skos:exactMatch <https://w3id.org/codemeta/3.0related_software/id> .

<https://swmath.org/vocabulary/codemeta-mapping/vocabrelated_software/name> skos:exactMatch <https://w3id.org/codemeta/3.0related_software/name> .

subject:source_code skos:exactMatch codemeta:source_code .

<subject:authors/codemeta:name> skos:exactMatch <codemeta:authors/codemeta:name> .

[] a owl:Axiom ;
    owl:annotatedProperty skos:closeMatch ;
    owl:annotatedSource subject:references_year_alt ;
    owl:annotatedTarget codemeta:references_year_alt ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "references_year_alt" ;
    sssom:subject_label "references_year_alt" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource <subject:authors/codemeta:name> ;
    owl:annotatedTarget <codemeta:authors/codemeta:name> ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "authors/codemeta:name" ;
    sssom:subject_label "authors/codemeta:name" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource <https://swmath.org/vocabulary/codemeta-mapping/vocabrelated_software/name> ;
    owl:annotatedTarget <https://w3id.org/codemeta/3.0related_software/name> ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "related_software/name" ;
    sssom:subject_label "related_software/name" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource subject:description ;
    owl:annotatedTarget schema1:description ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "description" ;
    sssom:subject_label "description" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource subject:source_code ;
    owl:annotatedTarget codemeta:source_code ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "source_code" ;
    sssom:subject_label "source_code" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:broader ;
    owl:annotatedSource subject:classification ;
    owl:annotatedTarget codemeta:classification ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "classification" ;
    sssom:subject_label "classification" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource subject:id ;
    owl:annotatedTarget codemeta:id ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "id" ;
    sssom:subject_label "id" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource subject:references ;
    owl:annotatedTarget codemeta:references ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "references" ;
    sssom:subject_label "references" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource <https://swmath.org/vocabulary/codemeta-mapping/vocabrelated_software/id> ;
    owl:annotatedTarget <https://w3id.org/codemeta/3.0related_software/id> ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "related_software/id" ;
    sssom:subject_label "related_software/id" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource subject:name ;
    owl:annotatedTarget schema1:name ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "name" ;
    sssom:subject_label "name" .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource subject:homepage ;
    owl:annotatedTarget codemeta:homepage ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "homepage" ;
    sssom:subject_label "homepage" .

[] a owl:Ontology ;
    dcterms:description """Mapping from the SWMATH software vocabulary to the CodeMeta 3.0 metadata standard covering key fields such as identifiers	 names	 descriptions	 URLs	 related software authors	 and citations to enable interoperability between the vocabularies.
""" ;
    dcterms:license "https://creativecommons.org/publicdomain/zero/1.0/"^^xsd:anyURI ;
    pav:authoredOn "2025-06-10"^^xsd:date ;
    sssom:mapping_provider "https://orcid.org/0000-0002-8710-9548"^^xsd:anyURI ;
    sssom:mapping_set_id "https://w3id.org/codemeta/crosswalks/swMATH"^^xsd:anyURI .


