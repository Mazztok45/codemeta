@prefix pav: <http://purl.org/pav/> .
@prefix schema1: <http://schema.org/> .
@prefix semapv: <https://w3id.org/semapv/vocab/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sssom: <https://w3id.org/sssom/> .
@prefix subject: <https://github.com/MaRDI4NFDI/swMATH4EOSC/blob/main/developer_documentation/schema_swMATH.xsd> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sssom:confidence a owl:AnnotationProperty .

sssom:mapping_justification a owl:AnnotationProperty .

sssom:object_label a owl:AnnotationProperty .

sssom:subject_label a owl:AnnotationProperty .

<https://github.com/MaRDI4NFDI/swMATH4EOSC/blob/main/developer_documentation/schema_swMATH.xsdauthors/name> skos:exactMatch <https://w3id.org/codemeta/3.0authors/name> .

subject:classification skos:broader codemeta:classification .

subject:dependencies skos:exactMatch codemeta:dependencies .

subject:description skos:exactMatch schema1:description .

subject:homepage skos:exactMatch codemeta:homepage .

subject:id skos:exactMatch codemeta:id .

subject:keywords skos:exactMatch schema1:keywords .

subject:name skos:exactMatch schema1:name .

subject:orms_id skos:exactMatch codemeta:orms_id .

subject:programming_languages skos:exactMatch codemeta:programming_languages .

subject:references skos:exactMatch codemeta:references .

subject:references_year_alt skos:closeMatch codemeta:references_year_alt .

<https://github.com/MaRDI4NFDI/swMATH4EOSC/blob/main/developer_documentation/schema_swMATH.xsdrelated_software/id> skos:exactMatch <https://w3id.org/codemeta/3.0related_software/id> .

<https://github.com/MaRDI4NFDI/swMATH4EOSC/blob/main/developer_documentation/schema_swMATH.xsdrelated_software/name> skos:exactMatch <https://w3id.org/codemeta/3.0related_software/name> .

subject:source_code skos:exactMatch codemeta:source_code .

subject:standard_articles skos:exactMatch codemeta:standard_articles .

subject:zbmath_url skos:exactMatch codemeta:zbmath_url .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource subject:zbmath_url ;
    owl:annotatedTarget codemeta:zbmath_url ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "zbmath_url" ;
    sssom:subject_label "zbmath_url" .

