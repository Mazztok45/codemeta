codemeta:funding skos:exactMatch target:awardNumber .

codemeta:givenName skos:exactMatch target:givenName .

codemeta:keywords skos:exactMatch target:subject .

codemeta:license skos:exactMatch <https://w3id.org/codemeta/crosswalks/datacite/vocabRights/rightURI> .

codemeta:programmingLanguage skos:narrower target:Format .

codemeta:publisher skos:exactMatch <https://w3id.org/codemeta/crosswalks/datacite/vocabPublisher/publisherIdentifier> .

codemeta:relatedLink skos:closeMatch target:RelatedIdentifier.relationType .

codemeta:version skos:exactMatch target:version .

codemeta:identifier skos:exactMatch target:identifier,
        target:nameIdentifier .

codemeta:name skos:exactMatch target:creatorName,
        target:title .

[] a owl:Axiom ;
    owl:annotatedProperty skos:exactMatch ;
    owl:annotatedSource codemeta:familyName ;
    owl:annotatedTarget target:familyName ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "familyName" ;
    sssom:object_source <target> ;
    sssom:subject_label "familyName" ;
    sssom:subject_source <schema> .

