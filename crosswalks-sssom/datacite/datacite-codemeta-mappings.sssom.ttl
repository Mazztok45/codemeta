@prefix dc1: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pav: <http://purl.org/pav/> .
@prefix schema1: <http://schema.org/> .
@prefix semapv: <https://w3id.org/semapv/vocab/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sssom: <https://w3id.org/sssom/> .
@prefix target: <https://w3id.org/codemeta/crosswalks/datacite/vocab> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sssom:confidence a owl:AnnotationProperty .

sssom:mapping_justification a owl:AnnotationProperty .

sssom:object_label a owl:AnnotationProperty .

sssom:subject_label a owl:AnnotationProperty .

schema1:affiliation skos:exactMatch target:affiliation .

schema1:author skos:exactMatch target:creators .

schema1:contributor skos:exactMatch target:contributor .

schema1:dateCreated skos:narrower target:Date .

schema1:dateModified skos:narrower target:Date .

schema1:datePublished skos:exactMatch target:publicationYear .

schema1:description skos:exactMatch target:description .

schema1:familyName skos:exactMatch target:familyName .

schema1:fileFormat skos:exactMatch target:Format .

schema1:fileSize skos:exactMatch target:size .

schema1:funder skos:closeMatch target:FundingReference .

schema1:givenName skos:exactMatch target:givenName .

schema1:keywords skos:exactMatch target:subject .

schema1:license skos:closeMatch target:Rights .

schema1:programmingLanguage skos:narrower target:Format .

schema1:publisher skos:closeMatch target:Publisher .

schema1:relatedLink skos:closeMatch target:RelatedIdentifier .

schema1:version skos:exactMatch target:version .

codemeta:funding skos:exactMatch target:awardNumber .

schema1:identifier skos:exactMatch target:identifier,
        target:nameIdentifier .

schema1:name skos:exactMatch target:creatorName,
        target:title .

[] a owl:Axiom ;
    owl:annotatedProperty skos:closeMatch ;
    owl:annotatedSource schema1:funder ;
    owl:annotatedTarget target:FundingReference ;
    sssom:confidence 1e+00 ;
    sssom:mapping_justification semapv:UnspecifiedMatching ;
    sssom:object_label "FundingReference" ;
    sssom:subject_label "funder" .

